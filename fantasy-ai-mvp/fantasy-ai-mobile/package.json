{
  "name": "fantasy-ai-mobile",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "start:clear": "expo start -c",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "dev": "expo start --dev-client",
    "build:android": "eas build --platform android",
    "build:android:apk": "eas build --platform android --profile preview",
    "build:android:local": "npx expo run:android --variant release",
    "build:ios": "eas build --platform ios",
    "build:all": "eas build --platform all",
    "dev:tunnel": "expo start --tunnel",
    "preview": "expo start --no-dev --minify",
    "prebuild": "expo prebuild",
    "prebuild:clean": "expo prebuild --clean",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "clean": "rm -rf node_modules .expo dist",
    "reset": "npm run clean && npm install && npm run start:clear"
  },
  "dependencies": {
    "expo": "~49.0.0",
    "expo-status-bar": "~1.6.0",
    "expo-splash-screen": "~0.20.5",
    "expo-font": "~11.4.0",
    "expo-linking": "~5.0.2",
    "expo-router": "^2.0.0",
    "expo-system-ui": "~2.4.0",
    "expo-web-browser": "~12.3.2",
    "expo-local-authentication": "~13.4.1",
    "expo-av": "~13.4.1",
    "expo-haptics": "~12.4.0",
    "expo-device": "~5.4.0",
    "expo-notifications": "~0.20.1",
    "expo-location": "~16.1.0",
    "expo-sensors": "~12.3.0",
    "expo-camera": "~13.4.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.72.6",
    "react-native-web": "~0.19.6",
    "react-native-gesture-handler": "~2.12.0",
    "react-native-reanimated": "~3.3.0",
    "react-native-screens": "~3.22.0",
    "react-native-safe-area-context": "4.6.3",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/bottom-tabs": "^6.5.8",
    "@react-navigation/stack": "^6.3.17",
    "@react-native-async-storage/async-storage": "1.18.2",
    "@tanstack/react-query": "^5.0.0",
    "react-native-svg": "13.9.0",
    "react-native-paper": "^5.10.0",
    "react-native-vector-icons": "^10.0.0",
    "lottie-react-native": "5.1.6",
    "react-native-linear-gradient": "^2.8.0",
    "react-native-shimmer-placeholder": "^2.0.9",
    "@shopify/flash-list": "^1.6.3",
    "react-native-fast-image": "^8.6.3",
    "react-native-mmkv": "^2.11.0",
    "react-native-background-fetch": "^4.2.1",
    "@sentry/react-native": "^5.19.0",
    "react-native-super-grid": "^5.0.0",
    "react-native-skeleton-placeholder": "^5.2.4",
    "react-native-haptic-feedback": "^2.2.0",
    "@react-native-community/netinfo": "^9.3.10",
    "@react-native-voice/voice": "^3.2.4",
    "expo-secure-store": "~12.3.1",
    "expo-constants": "~14.4.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.2.14",
    "@types/react-native": "^0.72.0",
    "typescript": "^5.1.3",
    "jest": "^29.2.1",
    "eslint": "^8.43.0",
    "eslint-config-expo": "^7.0.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "prettier": "^3.0.0"
  },
  "private": true
}
